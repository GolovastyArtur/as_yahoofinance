<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
        <!--  <langChildren>1</langChildren>!-->
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>General</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>

                    <settings.style>
                        <TCEforms>
                            <onChange>reload</onChange>
                            <label>LLL:EXT:as_yahoofinance/Configuration/FlexForms/locallang_db.xml:tt_content.as_yahoofinance.style</label>
                            <config>
                                <type>select</type>
                                <size>1</size>
                                <maxitems>1</maxitems>
                                <itemsProcFunc>asyahoofinance_itemsProcFunc->getStyle</itemsProcFunc>
                            </config>
                        </TCEforms>
                    </settings.style>

                    <settings.name>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>Name</label>
                            <config>
                                <type>input</type>
                                <size>20</size>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </settings.name>

                    <settings.days>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>Days</label>
                            <config>
                                <type>input</type>
                                <size>3</size>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </settings.days>

                    <settings.width>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>Width</label>
                            <config>
                                <type>input</type>
                                <size>4</size>
                            </config>
                        </TCEforms>
                    </settings.width>

                    <settings.height>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>Height</label>
                            <config>
                                <type>input</type>
                                <size>4</size>
                            </config>
                        </TCEforms>
                    </settings.height>
                    
                    <!--<settings.noChart>-->
                        <!--<TCEforms>-->
                            <!--<exclude>1</exclude>-->
                            <!--<displayCond>FIELD:settings.style:=:chart</displayCond>-->
                            <!--<label>No show Chart</label>-->
                            <!--<config>-->
                                <!--<type>check</type>-->
                            <!--</config>-->
                        <!--</TCEforms>-->
                    <!--</settings.noChart>-->

                    <settings.theme>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>Chart theme</label>
                            <config>
                                <type>select</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">custom</numIndex>
                                        <numIndex index="1">custom</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">light</numIndex>
                                        <numIndex index="1">light</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">patterns</numIndex>
                                        <numIndex index="1">patterns</numIndex>
                                    </numIndex>
                                    <numIndex index="3" type="array">
                                        <numIndex index="0">dark(um einen dunklen Hintergrund)</numIndex>
                                        <numIndex index="1">dark</numIndex>
                                    </numIndex>
                                    <numIndex index="4" type="array">
                                        <numIndex index="0">chalk(um einen dunklen Hintergrund)</numIndex>
                                        <numIndex index="1">chalk</numIndex>
                                    </numIndex>
                                </items>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                                <size>1</size>
                            </config>
                        </TCEforms>
                    </settings.theme>

                    <settings.positionY>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>Position Ordinate</label>
                            <config>
                                <type>select</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0"></numIndex>
                                        <numIndex index="1"></numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">left</numIndex>
                                        <numIndex index="1">left</numIndex>
                                    </numIndex>
                                    <numIndex index="2" type="array">
                                        <numIndex index="0">right</numIndex>
                                        <numIndex index="1">right</numIndex>
                                    </numIndex>
                                </items>
                                <minitems>0</minitems>
                                <maxitems>1</maxitems>
                                <size>1</size>
                            </config>
                        </TCEforms>
                    </settings.positionY>

                    <settings.showScrollbar>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>Show Scrollbar</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.showScrollbar>

                    <settings.fontSize>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>Font size</label>
                            <config>
                                <type>input</type>
                                <size>2</size>
                            </config>
                        </TCEforms>
                    </settings.fontSize>

                    <settings.fillColor>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>Fill color</label>
                            <config>
                                <type>input</type>
                                <size>6</size>
                                <wizards>
                                    <_PADDING>2</_PADDING>
                                    <color>
                                        <type>colorbox</type>
                                        <dim>12x12</dim>
                                        <tableStyle>border:solid 1px black;</tableStyle>
                                        <module type="array">
                                            <name>wizard_colorpicker</name>
                                            <urlParameters type="array">
                                                <mode>wizard</mode>
                                                <act>file</act>
                                            </urlParameters>
                                        </module>

                                        <JSopenParams>height=300,width=450,status=0,menubar=0,scrollbars=1
                                        </JSopenParams>
                                    </color>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.fillColor>

                    <settings.fontColor>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>Scale font color</label>
                            <config>
                                <type>input</type>
                                <size>6</size>
                                <wizards>
                                    <_PADDING>2</_PADDING>
                                    <color>
                                        <type>colorbox</type>
                                        <dim>12x12</dim>
                                        <tableStyle>border:solid 1px black;</tableStyle>
                                        <module type="array">
                                            <name>wizard_colorpicker</name>
                                            <urlParameters type="array">
                                                <mode>wizard</mode>
                                                <act>file</act>
                                            </urlParameters>
                                        </module>

                                        <JSopenParams>height=300,width=450,status=0,menubar=0,scrollbars=1
                                        </JSopenParams>
                                    </color>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.fontColor>

                    <settings.scrollbarSelectedBackground>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>Scrollbar Selected Background</label>
                            <config>
                                <type>input</type>
                                <size>6</size>
                                <wizards>
                                    <_PADDING>2</_PADDING>
                                    <color>
                                        <type>colorbox</type>
                                        <dim>12x12</dim>
                                        <tableStyle>border:solid 1px black;</tableStyle>
                                        <module type="array">
                                            <name>wizard_colorpicker</name>
                                            <urlParameters type="array">
                                                <mode>wizard</mode>
                                                <act>file</act>
                                            </urlParameters>
                                        </module>

                                        <JSopenParams>height=300,width=450,status=0,menubar=0,scrollbars=1
                                        </JSopenParams>
                                    </color>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.scrollbarSelectedBackground>

                    <settings.cursorColor>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>Cursor color</label>
                            <config>
                                <type>input</type>
                                <size>6</size>
                                <wizards>
                                    <_PADDING>2</_PADDING>
                                    <color>
                                        <type>colorbox</type>
                                        <dim>12x12</dim>
                                        <tableStyle>border:solid 1px black;</tableStyle>
                                        <module type="array">
                                            <name>wizard_colorpicker</name>
                                            <urlParameters type="array">
                                                <mode>wizard</mode>
                                                <act>file</act>
                                            </urlParameters>
                                        </module>

                                        <JSopenParams>height=300,width=450,status=0,menubar=0,scrollbars=1
                                        </JSopenParams>
                                    </color>
                                </wizards>
                            </config>
                        </TCEforms>
                    </settings.cursorColor>

                    <settings.showTooltips>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:chart</displayCond>
                            <label>Show Tooltips</label>
                            <config>
                                <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.showTooltips>

                    <!-- NOCHART -->

                    <settings.names>
                        <TCEforms>
                            <exclude>1</exclude>
                            <displayCond>FIELD:settings.style:=:nochart</displayCond>
                            <label>Names</label>
                            <config>
                                <type>input</type>
                                <size>20</size>
                                <eval>required</eval>
                            </config>
                        </TCEforms>
                    </settings.names>


                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
